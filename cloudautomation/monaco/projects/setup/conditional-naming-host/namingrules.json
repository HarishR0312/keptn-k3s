{
    "type": "HOST",
    "nameFormat": "{{ .name }}",
    "displayName": "{{ .name }}",
    "enabled": true,
    "rules": [
      {
        "key": {
          "attribute": "HOST_TAGS",
          "type": "STATIC"
        },
        "comparisonInfo": {
          "type": "TAG",
          "operator": "TAG_KEY_EQUALS",
          "value": {
            "context": "ENVIRONMENT",
            "key": "KeptnStage",
            "value" : "{{ .stage }}"
          },
          "negate": false
        }
      }
    ]
  }