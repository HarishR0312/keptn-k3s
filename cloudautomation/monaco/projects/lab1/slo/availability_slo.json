{
    "enabled": true,
    "name": "{{ .name }}",
    "description": "% of time {{ .name }} service is available based on synthetic test",
    "useRateMetric": true,
    "metricExpression": "(builtin:synthetic.browser.availability.location.total:splitBy())",
    "metricRate": "",
    "metricDenominator": "",
    "metricNumerator": "",
    "evaluationType": "AGGREGATE",
    "filter": "mzName(\"Tenant: tnt-{{ .tenant }}-svc\"),type(\"SYNTHETIC_TEST\")",
    "target": 95.00,
    "warning": 99.00,
    "timeframe": "-1d"
}