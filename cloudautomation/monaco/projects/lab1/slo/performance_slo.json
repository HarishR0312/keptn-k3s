{
    "enabled": true,
    "name": "{{ .name }}",
    "description": "% of requests handled by xxxx service faster than 500ms",
    "useRateMetric": true,
    "metricExpression": "(100)*(calc:service.tenant.responsetime.count.faster500ms:splitBy())/(builtin:service.requestCount.server:splitBy())",
    "metricRate": "",
    "metricDenominator": "",
    "metricNumerator": "",
    "evaluationType": "AGGREGATE",
    "filter": "mzName(\"Tenant: tnt-{{ .tenant }}-svc\"),type(\"SERVICE\"),tag(\"[Environment]DT_APPLICATION_ENVIRONMENT:production\")",
    "target": 85.00,
    "warning": 90.00,
    "timeframe": "-1d"
}